{
  "name": "module-dependents",
  "description": "Get the list of npm modules that depend on any other npm module.",
  "version": "0.1.4",
  "homepage": "https://github.com/doowb/module-dependents",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/module-dependents",
  "bugs": {
    "url": "https://github.com/doowb/module-dependents/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^3.0.2",
    "npm-api": "^0.4.10",
    "npm-api-dependents": "^1.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "is-plain-object": "^2.0.4",
    "mocha": "3.5.3",
    "right-pad-keys": "^0.1.2",
    "through2": "^2.0.3"
  },
  "keywords": [
    "api",
    "dependent",
    "dependents",
    "module",
    "module-dependents",
    "modules",
    "npm",
    "npm-api",
    "package",
    "package-dependents",
    "packages",
    "repo",
    "repo-dependents",
    "repos"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "npm-api",
        "npm-api-dependents"
      ]
    },
    "reflinks": [
      "npm-api",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
